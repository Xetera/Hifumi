{
	"parserOptions": {
	  "ecmaVersion": 6,
	  "sourceType": "module",
	  "ecmaFeatures": {
		"jsx": true
	  }
	},
	"env": {
	  "browser": true
	},
	"rules": {
	  "semi": 2,
	  "object-curly-spacing": [2, "always"],
	  "spaced-comment": 1,
	  "no-console": 1,
	  "no-var": 2,
	  "no-eval": 2,
	  "no-bitwise": 2,
	  "no-alert": 2,
	  "no-delete-var": 2,
	  "no-else-return": 2,
	  "no-labels": 2,
	  "no-magic-numbers": [2, {
		"ignoreArrayIndexes": true,
		"enforceConst": true
	  }],
	  "no-return-await": 2,
	  "eqeqeq": 2,
	  "prefer-arrow-callback": 2,
	  "prefer-const": 2,
	  "prefer-template": 1
	}
  }